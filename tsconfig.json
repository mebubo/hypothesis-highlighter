{
  "compilerOptions": {
    "module": "ES2022",
    "moduleResolution": "Node",
    "lib": [ "ES2022", "DOM", "DOM.Iterable" ],
    "outDir": "./dist",
    "target": "ES2022",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ]
}
